A,IAAA,yBAAA;AAAA,OADA,EAEA,UAAA,CAAA;AAAA,eAAA,CAAA,IAAA,CAAA,CAAA,MAAA,GAAA,EAAA,IAAA,wCAAA;AAAA,OAFA;AAfA,KAAA;AAoBA,GAxBA;AA0BA,EAAA,KAAA,EAAA,EA1BA;AA6BA,EAAA,OAAA,kCACA,UAAA,CAAA,CAAA,WAAA,CAAA,CADA;AAEA,IAAA,OAFA,mBAEA,GAFA,EAEA;AACA,aAAA,KAAA,KAAA,CAAA,GAAA,CAAA;AACA,KAJA;AAKA,IAAA,WALA,yBAKA;AACA,WAAA,KAAA,CAAA,cAAA;AACA,KAPA;AAQA,IAAA,UARA,wBAQA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA;AACA,UAAA,CAAA,KAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AAAA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AAAA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AAAA,UAAA,MAAA,GAAA,IAAA,CAAA,MAAA;AAAA,UAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,WAAA,KAAA,CAAA,cAAA;AACA,WAAA,SAAA,CAAA;AAAA,QAAA,IAAA,EAAA,IAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,QAAA,KAAA,EAAA,KAAA;AAAA,QAAA,MAAA,EAAA,MAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA;AAdA;AA7BA,CAAA","sourcesContent":["<template>\n  <v-card>\n    <v-toolbar dark color=\"primary\">\n      <v-spacer></v-spacer>\n      <v-btn icon dark @click=\"closeDialog\">\n        <v-icon>mdi-close</v-icon>\n      </v-btn>\n    </v-toolbar>\n    <v-card-title>\n      <span class=\"text-h5\">Оставить отзыв</span>\n    </v-card-title>\n    <v-card-text>\n      <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\n        <v-container>\n          <v-row>\n            <v-col cols=\"12\">\n              <v-text-field\n                label=\"Ваше имя*\"\n                outlined\n                required\n                v-model=\"name\"\n                :rules=\"nameRules\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\">\n              <v-text-field\n                label=\"Ваш email*\"\n                outlined\n                required\n                v-model=\"email\"\n                :rules=\"emailRules\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\">\n              <v-text-field\n                label=\"Ваш телефон*\"\n                outlined\n                required\n                v-model=\"phone\"\n                :rules=\"phoneRules\"\n              ></v-text-field>\n            </v-col>\n            <v-col cols=\"12\">\n              <v-textarea\n                label=\"Ваш отзыв*\"\n                outlined\n                required\n                v-model=\"review\"\n                :rules=\"reviewRules\"\n                class=\"mb-4\"\n              ></v-textarea>\n            </v-col>\n            <v-col cols=\"12\">\n              <v-slider\n                v-model=\"grade\"\n                :thumb-size=\"34\"\n                thumb-label=\"always\"\n                :tick-labels=\"ticksLabels\"\n                :min=\"0\"\n                :max=\"5\"\n                step=\"1\"\n                ticks=\"always\"\n                tick-size=\"4\"\n              >\n                <template v-slot:thumb-label=\"{ value }\">\n                  <span class=\"thumb\">{{ satisfactionEmojis[value] }}</span>\n                </template>\n              </v-slider>\n            </v-col>\n          </v-row>\n        </v-container>\n        <small>Поля отмеченные * обязательные для заполнения</small>\n      </v-form>\n    </v-card-text>\n    <v-card-actions>\n      <v-spacer></v-spacer>\n      <v-btn color=\"blue darken-1\" text @click=\"sendReview\"\n        >Отправить отзыв</v-btn\n      >\n    </v-card-actions>\n  </v-card>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\nexport default {\n  name: \"ReviewsForm\",\n\n  data() {\n    return {\n      valid: true,\n      name: \"\",\n      email: \"\",\n      phone: \"\",\n      review: \"\",\n      grade: \"\",\n      satisfactionEmojis: [\"🙁\", \"😐\", \"🙂\", \"😊\", \"😁\", \"😍\"],\n      ticksLabels: [\"0\", \"1\", \"2\", \"3\", \"4\", \"5\"],\n      nameRules: [(v) => !!v || \"Имя обязательное поле\"],\n      emailRules: [\n        (v) => !!v || \"Email обязательное поле\",\n        (v) => /.+@.+\\..+/.test(v) || \"Email должен быть валидным\",\n      ],\n      phoneRules: [(v) => !!v || \"Телефон обязательное поле\"],\n      reviewRules: [\n        (v) => !!v || \"Отзыв обязательное поле\",\n        (v) => (v && v.length > 10) || \"Отзыв должен быть не менее 10 символов\",\n      ],\n    };\n  },\n\n  props: {\n  },\n\n  methods: {\n    ...mapActions([\"addReview\"]),\n    emotion(val) {\n      return this.icons[val];\n    },\n    closeDialog() {\n      this.$emit(\"close-dialog\");\n    },\n    sendReview() {\n      const valid = this.$refs.form.validate();\n      if (!valid) return;\n      const { name, email, phone, review, grade } = this;\n      this.$emit(\"close-dialog\");\n      this.addReview({ name, email, phone, review, grade });\n    },\n  },\n};\n</script>\n\n<style>\n.thumb {\n  font-size: 24px;\n  width: 24px;\n  height: 24px;\n  line-height: 2;\n}\n</style>\n"],"sourceRoot":"src/components"}]}