AA,CAAA,gBAAA,GAAA,KAAA,EAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AAAA,OAAA,EAAA,IAAA,CAAA;AACA,KAPA;AASA,IAAA,kBATA,gCASA;AACA,UAAA,KAAA,MAAA,CAAA,QAAA,KAAA,EAAA,IAAA,KAAA,MAAA,CAAA,QAAA,KAAA,IAAA,EAAA;AACA,aAAA,aAAA,CAAA,QAAA,GAAA,4BAAA;AACA,eAAA,KAAA;AACA,OAHA,MAGA;AACA,aAAA,aAAA,CAAA,QAAA,GAAA,EAAA;AACA;;AACA,aAAA,IAAA;AACA,KAjBA;AAmBA,IAAA,mBAnBA,iCAmBA;AACA,UAAA,KAAA,MAAA,CAAA,SAAA,KAAA,EAAA,IAAA,KAAA,MAAA,CAAA,SAAA,KAAA,IAAA,EAAA;AACA,aAAA,aAAA,CAAA,SAAA,GAAA,4BAAA;AACA,eAAA,KAAA;AACA,OAHA,MAGA;AACA,aAAA,aAAA,CAAA,SAAA,GAAA,EAAA;AACA;;AACA,aAAA,IAAA;AACA,KA3BA;AA6BA,IAAA,eA7BA,6BA6BA;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,KAAA,EAAA,IAAA,KAAA,MAAA,CAAA,KAAA,KAAA,IAAA,EAAA;AACA,aAAA,aAAA,CAAA,KAAA,GAAA,kBAAA;AACA,eAAA,KAAA;AACA,OAHA,MAGA;AACA,aAAA,aAAA,CAAA,KAAA,GAAA,EAAA;AACA;;AACA,aAAA,IAAA;AACA,KArCA;AAuCA,IAAA,QAvCA,sBAuCA;AACA,aAAA,KAAA,kBAAA,MAAA,KAAA,eAAA,EAAA,IAAA,KAAA,mBAAA,EAAA;AACA;AAzCA;AA7BA,CAAA","sourcesContent":["<template v-slot:top>\n  <div>\n    <DefaultButton :button-info=\"sitter.buttonInfo\" @eventHandler=\"isReviewWindowOn = true\"  />\n    <v-dialog v-model=\"isReviewWindowOn\" max-width=\"950px\">\n      <v-form ref=\"form\" class=\"sitter-review-modal\">\n        <v-toolbar flat>\n          <v-toolbar-title>\n            Оцените работу ситтера\n          </v-toolbar-title>\n        </v-toolbar>\n        <v-card-text>\n            <v-textarea\n              color=\"#18a8b5\"\n              v-model=\"review.textarea\"\n              outlined\n              :label=\"textareaLabel\"\n              clearable\n              :error-messages=\"errorMessages.textarea\"\n              @input=\"checkTextareaField\"\n            />\n              <v-label>{{ radioLabel }}</v-label>\n              <v-radio-group\n                v-model=\"review.radio\"\n                row\n                :error-messages=\"errorMessages.radio\"\n                @change=\"checkRadioField\"\n              >\n                <v-radio\n                  v-for=\"n in 5\"\n                  :key=\"n\"\n                  :label=\"`${n}`\"\n                  color=\"#18a8b5\"\n                  :value=\"n\"\n                />\n              </v-radio-group>\n            <v-text-field\n              color=\"#18a8b5\"\n              v-model=\"review.textfield\"\n              class=\"mt-4\"\n              outlined\n              clearable\n              :label=\"textfieldLabel\"\n              :error-messages=\"errorMessages.textfield\"\n              @input=\"checkTextfieldField\"\n            />\n            <DefaultButton :button-info=\"sitter.buttonInfo\" @eventHandler=\"postReview\" />\n        </v-card-text>\n      </v-form>\n    </v-dialog>\n        <v-dialog\n      v-model=\"loading\"\n      persistent\n      width=\"300\"\n    >\n      <v-card\n        color=\"#18a8b5\"\n        dark\n      >\n        <v-card-text class=\"pt-2\">\n          Пожалуйста, подождите\n          <v-progress-linear\n            indeterminate\n            color=\"white\"\n            class=\"mt-2\"\n          ></v-progress-linear>\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n    <v-dialog\n      v-model=\"dialog\"\n      width=\"500\"\n    >\n      <v-card>\n        <v-card-title class=\"text-h5\">\n          Ваш отзыв отправлен\n        </v-card-title>\n\n        <v-card-text>\n          Спасибо!\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport DefaultButton from '../components/defaultButton'\n\nexport default {\n  components: { DefaultButton },\n  props: {\n    sitter: {\n      type: Object,\n      required: true\n    }\n  },\n  data () {\n    return {\n      textareaLabel: 'Отзыв о работе',\n      textfieldLabel: 'Ваше имя',\n      radioLabel: 'Оцените работу ситтера',\n      loading: false,\n      dialog: false,\n      isReviewWindowOn: false,\n      review: {\n        textarea: '',\n        radio: null,\n        textfield: ''\n      },\n      errorMessages: {\n        textarea: '',\n        textfield: '',\n        radio: ''\n      }\n    }\n  },\n\n  methods: {\n    postReview () {\n      if (!this.validate()) {\n        return false\n      }\n      this.loading = true\n      setTimeout(() => (this.loading = false, this.isReviewWindowOn = false,this.dialog = true), 2000)\n    },\n\n    checkTextareaField () {\n          if (this.review.textarea === '' || this.review.textarea === null) {\n            this.errorMessages.textarea = \"Поле должно быть заполнено\"\n            return false\n          } else {\n            this.errorMessages.textarea = ''\n          }\n      return true\n    },\n\n    checkTextfieldField () {\n          if (this.review.textfield === '' || this.review.textfield === null) {\n            this.errorMessages.textfield = \"Поле должно быть заполнено\"\n            return false\n          } else {\n            this.errorMessages.textfield = ''\n          }\n      return true\n    },\n\n    checkRadioField () {\n          if (this.review.radio === '' || this.review.radio === null) {\n            this.errorMessages.radio = \"Поставьте оценку\"\n            return false\n          } else {\n            this.errorMessages.radio = ''\n          }\n      return true\n    },\n\n    validate () {\n      return this.checkTextareaField() && this.checkRadioField() && this.checkTextfieldField()\n    }\n  }\n}\n</script>"],"sourceRoot":"src/components"}]}