A,GAAA,KAAA,OAAA,CAAA,MAAA,CAAA,UAAA,MAAA;AAAA,iBAAA,eAAA,CAAA,WAAA,CAAA,IAAA,CAAA,UAAA,SAAA;AAAA,mBAAA,MAAA,CAAA,WAAA,CAAA,QAAA,CAAA,SAAA,CAAA;AAAA,WAAA,CAAA;AAAA,SAAA,CAAA;;AACA,aAAA,aAAA,GAAA,OAAA;AACA;;AAEA,UAAA,eAAA,CAAA,SAAA,CAAA,MAAA,GAAA,CAAA,IAAA,eAAA,CAAA,WAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,YAAA,QAAA,GAAA,KAAA,OAAA,CAAA,MAAA,CAAA,UAAA,MAAA;AAAA,iBAAA,eAAA,CAAA,SAAA,CAAA,IAAA,CAAA,UAAA,QAAA;AAAA,mBAAA,MAAA,CAAA,SAAA,CAAA,QAAA,CAAA,QAAA,CAAA;AAAA,WAAA,CAAA;AAAA,SAAA,CAAA;;AACA,aAAA,aAAA,GAAA,QAAA;AACA;;AAEA,UAAA,eAAA,CAAA,SAAA,CAAA,MAAA,KAAA,CAAA,IAAA,eAAA,CAAA,WAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,aAAA,aAAA,GAAA,KAAA,OAAA;AACA;AACA,KA5BA;AA6BA,IAAA,YA7BA,wBA6BA,MA7BA,EA6BA;AACA,UAAA,MAAA,KAAA,aAAA,EAAA;AACA,aAAA,aAAA,GAAA,KAAA,mBAAA,EAAA;AACA;;AACA,UAAA,MAAA,KAAA,UAAA,EAAA;AACA,aAAA,aAAA,GAAA,KAAA,gBAAA,EAAA;AACA;AACA,KApCA;AAqCA,IAAA,mBArCA,iCAqCA;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,aAAA,CAAA,EAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,MAAA,GAAA,CAAA,CAAA,MAAA;AACA,OAFA,CAAA;AAGA,KAzCA;AA0CA,IAAA,gBA1CA,8BA0CA;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,KAAA,aAAA,CAAA,EAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,GAAA,GAAA,CAAA,CAAA,GAAA;AACA,OAFA,CAAA;AAGA;AA9CA;AArEA,CAAA","sourcesContent":["<template>\n    <div class=\"container\">\n    <div class=\"mt-6 mb-6\">\n    <h1 class=\"it-title\">\n      {{ title }}\n    </h1>\n    <p class=\"it-desc\">\n      {{ desc }}\n    </p>\n    </div>\n    <div class=\"row mt-12\">\n      <div class=\"col filtration_block\">\n        <SittersFilters @filterList=\"filterList\"/>\n      </div>\n      <div class=\"col\">\n        <FilterSelect :filters=\"filters\" @changeFilter=\"changeFilter\" />\n        <SittersList :sitters=\"sortedSitters\" />\n      </div>\n    </div>\n    <div class=\"mt-6 mb-6\">      \n      <Pagination :page=\"page\" :count=\"count\" :page-size=\"pageSize\" @updatePage=\"updatePage\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport SittersFilters from '../components/sittersFilters'\nimport SittersList from '../components/sittersList'\nimport Pagination from '../components/pagination'\nimport FilterSelect from '../components/filterSelect'\n\n\nexport default {\n  components: { SittersFilters, SittersList, Pagination, FilterSelect },\n  data () {\n    return {\n      filters: ['По рейтингу', 'По стажу'],\n      page: 1,\n      count: 20,\n      pageSize: 10,\n      title: 'Наши ситтеры',\n      sortedSitters: [],\n      desc: 'Найдите подходящего под Ваши критерии ситтера для Вашего питомца',\n      sitters: [\n        {\n          name: 'Jane Foster',\n          image: 'jane-foster.jpeg',\n          id: 213,\n          rating: 3,\n          exp: 2,\n          buttonInfo: {\n            text: 'Посмотреть профиль'\n          },\n          cityAreas: ['Юг'],\n          specialties: ['Собаки'],\n          info: [\n            { icon: 'mdi-calendar-range', text: 'Стаж', value: '2 года' },\n            { icon: 'mdi-map-marker', text: 'Район города', value: 'Юг' },\n            { icon: 'mdi-paw', text: 'Специализация', value: 'Собаки' },\n          ]\n        },\n        {\n          name: 'Alex Rakatanski',\n          image: 'alex-rakatanski.jpeg',\n          id: 214,\n          rating: 5,\n          exp: 1,\n          buttonInfo: {\n            text: 'Посмотреть профиль'\n          },\n          cityAreas: ['Центр'],\n          specialties: ['Кошки'],\n          info: [\n            { icon: 'mdi-calendar-range', text: 'Стаж', value: '1 год' },\n            { icon: 'mdi-map-marker', text: 'Район города', value: 'Центр' },\n            { icon: 'mdi-paw', text: 'Специализация', value: 'Кошки' },\n          ]\n        },\n        {\n          name: 'Clay Rideaux',\n          image: 'clay-rideaux.jpeg',\n          id: 215,\n          rating: 4,\n          exp: 5,\n          buttonInfo: {\n            text: 'Посмотреть профиль'\n          },\n          cityAreas: ['Север'],\n          specialties: ['Экзотические животные'],\n          info: [\n            { icon: 'mdi-calendar-range', text: 'Стаж', value: '5 лет' },\n            { icon: 'mdi-map-marker', text: 'Район города', value: 'Север' },\n            { icon: 'mdi-paw', text: 'Специализация', value: 'Экзотические животные' },\n          ]\n        },\n      ]\n    }\n  },\n  mounted() {\n    this.sortedSitters = this.sitters\n  },\n  methods: {\n    updatePage (localPageSelected) {\n      this.page = localPageSelected\n      this.getSittersList()\n    },\n    getSittersList () {\n        //запрос на сервер\n    },\n    filterList (optionsSelected) {\n\n      if (optionsSelected.cityAreas.length > 0 && optionsSelected.specialties.length > 0 ) {\n      let result = this.sitters.filter( sitter => optionsSelected.cityAreas.some( cityArea => sitter.cityAreas.includes(cityArea) ) && optionsSelected.specialties.some( specialty => sitter.specialties.includes(specialty) )  );\n      this.sortedSitters = result\n      }\n\n      if (optionsSelected.cityAreas.length === 0 && optionsSelected.specialties.length > 0 ) {\n      let result = this.sitters.filter( sitter => optionsSelected.specialties.some( specialty => sitter.specialties.includes(specialty) )  );\n      this.sortedSitters = result\n      }\n\n      if (optionsSelected.cityAreas.length > 0 && optionsSelected.specialties.length === 0 ) {\n      let result = this.sitters.filter( sitter => optionsSelected.cityAreas.some( cityArea => sitter.cityAreas.includes(cityArea) )  );\n      this.sortedSitters = result\n      }\n\n      if (optionsSelected.cityAreas.length === 0 && optionsSelected.specialties.length === 0 ) {\n      this.sortedSitters = this.sitters\n      }\n    },\n    changeFilter (filter) {\n      if (filter === 'По рейтингу') {\n        this.sortedSitters = this.sortedSittersRating()\n      }\n      if (filter === 'По стажу') {\n        this.sortedSitters = this.sortedSittersExp()\n      }\n    },\n    sortedSittersRating () {\n      return JSON.parse(JSON.stringify(this.sortedSitters)).sort(function (a, b) {\n        return b.rating - a.rating\n      })\n    },\n    sortedSittersExp () {\n      return JSON.parse(JSON.stringify(this.sortedSitters)).sort(function (a, b) {\n        return b.exp - a.exp\n      })\n    }\n  }\n\n}\n</script>\n"],"sourceRoot":"src/pages"}]}